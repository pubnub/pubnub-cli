{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "lib",
        "sourceMap": true,
        "target": "es6",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
        "types": [
            "node"
        ]
    }
}
