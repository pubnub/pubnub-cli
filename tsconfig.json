{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "outDir": "./lib",
        "rootDir": "./src",
        "lib": [
            "ES2015"
        ],
        "alwaysStrict": true,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true
    }
}
